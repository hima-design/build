/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "resolveJsonModule": true
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts",
    "node_modules/@nebular/**/*.spec.ts"
  ],
  "include": [
    "src/**/*.d.ts",
    "src/*.ts",
    "node_modules/@nebular/**/*.ts"
  ]
}
